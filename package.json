{
	"name": "my-app",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migrate:make": "knex --knexfile knexfile.cjs migrate:make",
		"seed:make": "knex --knexfile knexfile.cjs seed:make --stub ./stub/seed.cjs.stub",
    "migrate": "knex --knexfile knexfile.cjs migrate:latest",
    "rollback": "knex --knexfile knexfile.js migrate:rollback",
    "seed": "knex --knexfile knexfile.cjs seed:run $*"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcrypt": "^5.0.2",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.47",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"knex": "^3.1.0",
		"pg": "^8.13.0",
		"postgres": "^3.4.4"
	}
}
